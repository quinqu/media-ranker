
<table>
  <tr> 
    <th> Username </th>
    <th> Votes </th>
    <th> Join Date </th>
  </tr>
    <%@user.each do |u| %>
    <tr>
      <td> <%= link_to u.username, user_path(u.id) %></td>
      <td> <%= u.votes.length %></td>
      <td> <%= u.join_date %></td>
     
    </tr>
  <%end %>
</table>

