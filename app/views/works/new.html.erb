
<h1> New Media </h1>

<% if @work.errors.any? %>
  <ul>
    <% @work.errors.each do |column, message| %>
      <li>
        <strong> <%= column.capitalize %> </strong> <%= message %>
      </li>
    <% end %>
  </ul>
<% end %>
<%= render partial: "form", locals: { action_name: "New", title: "", category: "", year: "", description: "", creator: "" }  %>

